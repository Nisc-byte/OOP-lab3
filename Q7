#include <iostream>
using namespace std;

class Distance {
    int meter;
    int centimeter;
public:
    Distance(int m = 0, int cm = 0) {
        meter = m;
        centimeter = cm;
    }
    bool operator<=(const Distance& d) const {
        if (meter < d.meter) return true;
        if (meter == d.meter && centimeter <= d.centimeter) return true;
        return false;
    }
    void display() const {
        cout << meter << " m " << centimeter << " cm" << endl;
    }
};

int main() {
    Distance d1(5, 75), d2(6, 10);
    
    cout << "Distance 1: ";
    d1.display();
    cout << "Distance 2: ";
    d2.display();
    
    if (d1 <= d2)
        cout << "Distance 1 is less than or equal to Distance 2" << endl;
    else
        cout << "Distance 1 is greater than Distance 2" << endl;
    
    return 0;
}
